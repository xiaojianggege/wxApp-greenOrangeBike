/* components/topNav/topNav.wxss */
.place-holder{
  width: 100%;
  height: 130rpx;
}
.nav-bar{
  position: fixed;
  left: 0;
  top: 0;
  box-sizing: border-box;
  width: 100%;
  height: 130rpx;
  background: #00DEC1;
  padding: 52rpx 0 10rpx 25rpx;
}
.nav-bar_title{
  display: flex;
  height: 100%;
  width: 100%;
  color: #fff;
  align-items: center;
  
}
.nav-bar_title text{
  font-size: 26rpx;
  line-height: 100%;
  letter-spacing: 1rpx;
}