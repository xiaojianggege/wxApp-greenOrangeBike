.container{
    margin:50rpx;
    padding: 0;
}
.useCar{
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.line{
    flex: 1;
    display: block;
    text-align: center;
    color: #E5E5E5;
}
.title{
    font-size: 35rpx;
    font-weight: 100rpx;
    color: #999999;
}
.case{
    width: 100%;
    display: flex;
}
.price{
    flex: 1;
    font-size:35rpx;
    font-weight: 100rpx;
    color: #6E6E6E;
}
.text-align{
    text-align: right;
}
.rule{
    flex: 1;
    margin-top: 20rpx;
    font-size:28rpx;
    font-weight: 100rpx;
    color: #999999;
    letter-spacing: 3rpx;
}
.explain{
    margin-top: 20rpx;
    font-size:30rpx;
    font-weight: 100rpx;
    color: #999999;
}
.blank{
    height: 80rpx;
    color: #E5E5E5;
    line-height: 80rpx;
}
.BuyCard{
    margin-top: 60rpx;
    width: 400rpx;
    height: 70rpx;
    color: #6E6E6E;
    text-align: center;
    border: 1rpx solid #6E6E6E;
    line-height: 70rpx;
    border-radius: 12rpx;
}